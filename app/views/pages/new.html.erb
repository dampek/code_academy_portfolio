<h1>CREATE A NEW JOB</h1>

<div class="create">
  <div class="container">
    
<%= form_for(@page) do |f| %>  
  <div class="field"> 
    <%= f.label :page %><br> 
    <%= f.text_area :cover %> 
    <%= f.text_area :title %> 
    <%= f.text_area :artist %> 
  </div> 
  <div class="actions"> 
    <%= f.submit "Create" %> 
  </div> 
<% end %>
    
  </div>
</div>